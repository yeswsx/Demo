<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.wsx.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RxJavaActivity"/>
        <activity android:name=".TestMvpActivity"/>
        <activity android:name=".RetrofitActivity">
        </activity>
        <activity android:name=".KotlinActivity">
        </activity>
        <activity
            android:name=".SecondActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="sensor">
        </activity>
        <activity
            android:name=".NewContextActivity"
            android:screenOrientation="sensor">
        </activity>
        <activity android:name=".event.EventBusActivity">
        </activity>
    </application>

</manifest>